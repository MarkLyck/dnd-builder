{
  "name": "dnd-turbo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "gc": "gitmoji -c",
    "gca": "git add . && pnpm run gc",
    "gcap": "pnpm run gca && git push"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "gitmoji-cli": "^7.0.2",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.13.4"
}
